{
  "name": "structlint",
  "description": "Lint your project structure against architecture rules",
  "keywords": [
    "structure linter",
    "architecture linter",
    "lint folders",
    "eslint for structure",
    "directory",
    "structure",
    "linter",
    "linting",
    "folder",
    "directories",
    "folders"
  ],
  "repository": "desfero/structlint",
  "bin": "./bin/structlint.js",
  "version": "0.3.1",
  "main": "index.js",
  "license": "MIT",
  "author": "Pawel Lula (desfero)",
  "scripts": {
    "tsc": "tsc --noEmit",
    "test": "jest",
    "lint": "cd ./src && structlint",
    "format": "prettier --check '**/*.{ts,tsx,js,json,md,gql}'",
    "format:fix": "prettier --write '**/*.{ts,tsx,js,json,md,gql}'"
  },
  "dependencies": {
    "@babel/parser": "^7.7.7",
    "@babel/traverse": "^7.7.4",
    "chalk": "^3.0.0",
    "cosmiconfig": "^6.0.0",
    "debug": "^4.1.1",
    "dedent": "^0.7.0",
    "globby": "^10.0.1",
    "linguist-languages": "^7.7.0",
    "lodash": "^4.17.15",
    "micromatch": "^4.0.2",
    "tslib": "^1.10.0",
    "ts-node": "^8.5.4",
    "yup": "^0.28.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-transform-modules-commonjs": "^7.7.5",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-typescript": "^7.7.7",
    "@types/babel__traverse": "^7.0.8",
    "@types/debug": "^4.1.5",
    "@types/dedent": "^0.7.0",
    "@types/lodash": "^4.14.149",
    "@types/micromatch": "^3.1.1",
    "@types/node": "^13.1.1",
    "@types/yup": "^0.26.27",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "structlint": "^0.3.1",
    "typescript": "^3.7.4"
  },
  "engines": {
    "node": ">=8"
  }
}
